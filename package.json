    "eslint": "9.39.1",
    "globals": "16.5.0",
    "prettier": "3.7.4",
    "prettier-plugin-organize-imports": "4.3.0",
    "typescript": "5.9.3",
    "typescript-eslint": "8.49.0",
    "vitest": "3.2.4"
  },
  "private": true,
  "volta": {
    "node": "22.21.1",
    "npm": "11.6.4"
