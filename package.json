    "@vitest/coverage-v8": "2.1.9",
    "eslint": "9.22.0",
    "globals": "15.15.0",
    "prettier": "3.5.3",
    "prettier-plugin-organize-imports": "4.1.0",
    "typescript": "5.8.2",
    "typescript-eslint": "8.26.1",
    "vitest": "2.1.9"
  },
  "private": true,
  "volta": {
    "node": "20.18.3",
    "npm": "10.9.2"
  }
